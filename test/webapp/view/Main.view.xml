<mvc:View
	controllerName="test.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	core:require="{
		formatter: 'test/model/formatter'
	}">

	<Page
		title="{i18n>appTitle}"
		id="page">
		<headerContent>
			<Button press="openAbicsAccessibilityPopover" icon="sap-icon://accessibility" />
		</headerContent>
		<content>
			<IllustratedMessage
				title="{i18n>appTitle}"
				illustrationType="sapIllus-SuccessHighFive"
				enableVerticalResponsiveness="true"
				description="{i18n>appDescription}">
			</IllustratedMessage>
		
		</content>
	</Page>

</mvc:View>
